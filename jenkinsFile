agent any
stages
{
        stage ('Build')
        {
            Steps{
		echo ('Building the project . . .')
        }
        }
        stage ('Test')
        {
Steps{
            echo ('Testing the project . . .')
}
        }
        
        stage ('Deploy')
        {
Steps{
            echo ('Deploying the project . . .')
}
        }
        
        stage ('Release')
        {
Steps{            
echo ('Releasing the project . . .')
}
        }
}
}
